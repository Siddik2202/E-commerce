      var analyticsLoaded = setInterval(function () {
        if (readCookie("isproteus") == "true" || (typeof analytics != "undefined" && typeof mixpanel != "undefined" && typeof mixpanel.get_distinct_id != "undefined")) {
          TrackEvents(eventName, eventProps, DFW.userId);
          clearInterval(analyticsLoaded);
        }
