    if (DFW.isTestBed || readCookie("isproteus") == "true" || (typeof analytics != "undefined" && typeof mixpanel != "undefined" && typeof mixpanel.get_distinct_id != "undefined")) {
      TrackEvents(eventName, eventProps, DFW.userId);
